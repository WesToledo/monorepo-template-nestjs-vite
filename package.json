{
  "name": "monorepo_template_nestjs_vite",
  "packageManager": "npm@11.3.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "dev:backend": "turbo dev --filter=backend",
    "dev:frontend": "turbo dev --filter=frontend",
    "build": "turbo build",
    "lint": "turbo lint",
    "test": "turbo test",
    "format": "turbo format",
    "db:migrate": "npm run prisma:migrate -w backend",
    "db:generate": "npm run prisma:generate -w backend",
    "db:studio": "npm run prisma:studio -w backend",
    "docker:up": "docker compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker compose -f docker-compose.dev.yml down"
  },
  "devDependencies": {
    "turbo": "^2.4.2"
  }
}
